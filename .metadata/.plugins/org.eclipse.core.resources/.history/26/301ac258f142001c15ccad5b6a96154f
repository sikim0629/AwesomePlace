	<script language="JavaScript1.2">
function show_list(val)
{
	location.href = "/help/faq.do?code=" + val;
}

function showFaq(id)
{
	var i;
	var cur_display;
	var cur_ele;
	var ele;
	
	cur_ele = document.all["faq" + id];
	cur_display = cur_ele.style.display;

	for (i = 0; i < 50; i++)
	{
		ele = document.all["faq" + i];
		if (ele != null)
		{
			if (i != id)
			{
				ele.style.display = "none";
			}
		}
	}

	if (cur_display == "none")
		cur_ele.style.display = "";
	else
		cur_ele.style.display = "none";
}

function goPage(page) {
	document.f.currPage.value = page;
	document.f.submit();
}

function golike() {
	
	ptype = document.faq.searchCon.value;
	like = document.faq.searchTxt.value;

	
	if( ptype == ""){
		alert("분류를 선택하십시요.");
		return;
	}
	if( like == ""){
		alert("검색명을 입력하십시요.");
		return;
	}
	document.faq.submit();
}
</script>
